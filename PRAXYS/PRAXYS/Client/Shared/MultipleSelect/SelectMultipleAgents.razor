@inherits SelectMultipleAgentsBase

@if (ItemsNotSelected != null)
{
    <MultipleSelect NoSelectItems="ItemsNotSelected" SelectItems="SelectedItems"></MultipleSelect>
}

<div class="mt-2 d-flex justify-content-end">
    <button class="btn btn-blue" @onclick="SaveSelection">Seleccionar</button>
</div>

@if (ReportFilter.Agents!= null)
{
    <div>AGENTES @ReportFilter.Agents.Count</div>
}